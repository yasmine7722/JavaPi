      <Label layoutX="41.0" layoutY="243.0" text="date" />
      <TableView fx:id="tab_m" layoutX="369.0" layoutY="43.0" onMouseClicked="#getSelected" prefHeight="200.0" prefWidth="200.0">
         <columns>
            <TableColumn fx:id="nom" prefWidth="75.0" text="nom" />
            <TableColumn fx:id="date" prefWidth="75.0" text="date" />
         </columns>
      </TableView>
      <Button fx:id="Btn_ajouter_m" layoutX="150.0" layoutY="302.0" mnemonicParsing="false" onAction="#ajout_m" text="ajouter" />
      <Button fx:id="Btn_modifier_m" layoutX="259.0" layoutY="302.0" mnemonicParsing="false" onAction="#modif_m" text="modifier" />
      <Button fx:id="Btn_supprimer_m" layoutX="368.0" layoutY="302.0" mnemonicParsing="false" onAction="#supprimer_m" text="supprimer" />
      <DatePicker fx:id="tfdate_m" layoutX="119.0" layoutY="238.0" prefHeight="31.0" prefWidth="187.0" promptText=" Entrez la date de fin " />
